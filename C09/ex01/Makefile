$NAME= libft.a
$CC = gcc
$FLAG = -Wall -Wextra -Werror
$SRC=

ALL:
	$(CC $(FLAG)) -o  $(NAME)

CLEAN:
	rm -f $(OBJS) $(NAME)

RE:
	make fclean && make
